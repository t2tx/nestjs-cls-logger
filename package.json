{
  "name": "nestjs-cls-logger",
  "version": "1.1.0",
  "description": "nestjs cls logger npm",
  "author": "t2tx <t2tx@icloud.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "readmeFilename": "README.md",
  "files": [
    "dist/**/*",
    "*.md",
    "LICENSE"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint -p \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "logger",
    "cls",
    "winston",
    "morgan"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/t2tx/nestjs-cls-logger.git"
  },
  "bugs": "https://github.com/t2tx/nestjs-cls-logger/issues",
  "peerDependencies": {
    "@nestjs/common": "> 8.0.0",
    "@nestjs/core": "> 8.0.0",
    "@nestjs/config": "> 3.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-slack-webhook-transport": "^2.3.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "@types/uuid": "^9.0.6",
    "@types/morgan": "^1.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "license": "MIT"
}